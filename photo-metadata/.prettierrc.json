{
  "bracketSpacing": true,
  "printWidth": 80,
  "proseWrap": "preserve",
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "tabWidth": 5,
  "useTabs": true,
  "parser": "typescript",
  "arrowParens": "avoid",
  "requirePragma": false,
  "insertPragma": false,
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": ".*rc",
      "options": {
        "singleQuote": false,
        "parser": "json"
      }
    }
  ]
}
