{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev:build": "tsc -p tsconfig-build.json",
    "dev:watch": "nodemon --exec \"pnpm run dev:build && pnpm run dev:start\" --watch src --ext ts",
    "dev:start": "node ./dist/index.js",
    "lib:up": "pnpm update --latest",
    "lint": "eslint '{src,test}/**/*.ts'",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "lint:fix": "eslint --fix '{src,test}/**/*.ts'",
    "test": "jest --watchAll --collectCoverage"
  },
  "watch": ["src/"],
  "ignore": [".git", "node_modules/", "dist/", "coverage/"],
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.3",
    "mongodb": "^5.1.0",
    "mongoose": "^7.0.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "zod": "^3.21.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "kafka-node": "^5.0.0",
    "kafkajs": "^2.2.4"
  },
  "env": {
	"NODE_ENV": "development"
  },
  "ext": "js,json,ts"
}
